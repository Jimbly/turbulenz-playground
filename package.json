{
  "name": "glovjs",
  "version": "0.1.0",
  "description": "Template for a JavaScript game project using GLOV.js",
  "main": "server/index.js",
  "keywords": [
    "template",
    "glov",
    "glovjs",
    "glov-build",
    "browserify",
    "babel"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Jimbly/glovjs.git"
